<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>log</Title>
			<Shortcut>log</Shortcut>
			<Description>Code snippet for writing to console using Console.WriteLine</Description>
			<Author>Jos Wigchert</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>var</ID>
					<ToolTip>variable to log to console</ToolTip>
					<Default>MyVar</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[Console.WriteLine($var$);$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>